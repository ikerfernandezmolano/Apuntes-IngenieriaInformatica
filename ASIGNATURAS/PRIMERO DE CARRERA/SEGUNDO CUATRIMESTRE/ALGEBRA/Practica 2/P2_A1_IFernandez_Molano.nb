Cell[TextData[{
 "Es un sistema compatible determinado para todo k si pertenece a ",
 StyleBox["R",
  FontWeight->"Bold"],
 " donde la soluci\[OAcute]n es:\nx=2\ny=-1-k\nz=k"
}], "Commentary",
 CellChangeTimes->{{3.9181117335537252`*^9, 3.9181117892319293`*^9}, {
  3.9181125817115574`*^9, 
  3.918112632464588*^9}},ExpressionUUID->"ef81e150-115e-4ca8-a3b0-\
a813c6fe4acc"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Reduce", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"3", "x"}], "-", 
       RowBox[{"2", "y"}], "+", "z"}], "==", "2"}], ",", 
     RowBox[{
      RowBox[{"x", "-", "y", "+", "z"}], "==", "1"}], ",", 
     RowBox[{
      RowBox[{"x", "+", "y", "+", 
       RowBox[{"k", " ", "z"}]}], "==", "2"}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"x", ",", "y", ",", "z"}], "}"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.9181117975122557`*^9, 3.9181118239869213`*^9}, 
   3.918112049811908*^9},
 CellLabel->
  "In[124]:=",ExpressionUUID->"1c3e2df1-4826-4b49-af00-7b225b0eecec"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"3", "+", "k"}], "\[NotEqual]", "0"}], "&&", 
  RowBox[{"x", "\[Equal]", 
   FractionBox["3", 
    RowBox[{"3", "+", "k"}]]}], "&&", 
  RowBox[{"y", "\[Equal]", 
   RowBox[{
    RowBox[{"-", "1"}], "+", 
    RowBox[{"2", " ", "x"}]}]}], "&&", 
  RowBox[{"z", "\[Equal]", "x"}]}]], "Output",
 CellChangeTimes->{{3.918111825184478*^9, 3.918111841842442*^9}, 
   3.91811205331429*^9},
 CellLabel->
  "Out[124]=",ExpressionUUID->"6715556a-3e80-463c-ae8c-95748944b02d"]
}, Open  ]],

Cell["\<\
Si k/=-3 entonces el sistema sera un sistema compatible determinado donde:
x=3/(3+k)
y=-1+2(3/(3+k))
z=3/(3+k)
Si k=3 entonces el sistema es incompatible
\
\>", "Commentary",
 CellChangeTimes->{{3.918112080993035*^9, 
  3.918112229071362*^9}},ExpressionUUID->"0e657948-44db-4d62-a539-\
901dc63c78b2"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Reduce", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{"x", "+", 
       RowBox[{"2", "y"}], "+", "z"}], "==", "1"}], ",", 
     RowBox[{
      RowBox[{
       RowBox[{"2", "x"}], "+", "y", "+", 
       RowBox[{"3", "z"}]}], "==", 
      RowBox[{"-", "4"}]}], ",", 
     RowBox[{
      RowBox[{"x", "-", "y", "+", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"a", "+", "2"}], ")"}], "z"}]}], "==", 
      RowBox[{
       RowBox[{
        RowBox[{"-", "3"}], "a"}], "-", "5"}]}], ",", 
     RowBox[{
      RowBox[{
       RowBox[{"4", "x"}], "+", 
       RowBox[{"2", "y"}], "+", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"a", "+", "6"}], ")"}], "z"}]}], "==", 
      RowBox[{
       RowBox[{
        RowBox[{"-", "3"}], 
        RowBox[{"a", "^", "2"}]}], "-", "8"}]}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"x", ",", "y", ",", "z"}], "}"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.9181118496695814`*^9, 3.9181119104672947`*^9}},
 CellLabel->
  "In[123]:=",ExpressionUUID->"75ad50de-4cbc-4741-b7b8-6034a6905c57"],

Cell[BoxData[
 RowBox[{
  RowBox[{"(", 
   RowBox[{
    RowBox[{"a", "\[Equal]", "0"}], "&&", 
    RowBox[{"y", "\[Equal]", 
     FractionBox[
      RowBox[{"7", "-", "x"}], "5"]}], "&&", 
    RowBox[{"z", "\[Equal]", 
     RowBox[{
      RowBox[{"-", 
       FractionBox["3", "5"]}], " ", 
      RowBox[{"(", 
       RowBox[{"3", "+", "x"}], ")"}]}]}]}], ")"}], "||", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{"a", "\[Equal]", "1"}], "&&", 
    RowBox[{"x", "\[Equal]", "2"}], "&&", 
    RowBox[{"y", "\[Equal]", "1"}], "&&", 
    RowBox[{"z", "\[Equal]", 
     RowBox[{"-", "3"}]}]}], ")"}]}]], "Output",
 CellChangeTimes->{3.918111910966541*^9},
 CellLabel->
  "Out[123]=",ExpressionUUID->"b5cdb260-a553-4903-b664-9080595ef170"]
}, Open  ]],

Cell["\<\
Para a=0, el sistema es un sistema compatible indeterminado donde:
x=x
y=(7-x)/5
z=-3/5*(3+x)
Para a=1, el sistema es un sistema compatible determinado donde:
x=2
y=1
z=-3
Para a/=0 y a/=0, el sistema es incompatible\
\>", "Commentary",
